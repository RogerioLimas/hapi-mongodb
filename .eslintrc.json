{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "single"],
        "no-console": "off",
        "linebreak-style": "off",
        "no-alert": "off",
        "indent": "off",
        "consistent-return": "off"
    }
}